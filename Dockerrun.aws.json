{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "anuragkumarak1995/multi-client",
			"hostname": "client",
			"essential": false
		},
		{
			"name": "server",
			"image": "anuragkumarak1995/multi-server",
			"hostname": "api",
			"essential": false
		},
		{
			"name": "worker",
			"image": "anuragkumarak1995/multi-worker",
			"essential": false
		},
		{
			"name": "nginx",
			"image": "anuragkumarak1995/multi-nginx",
			"postMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["client", "server"],
			"essential": true
		}
	]
}
